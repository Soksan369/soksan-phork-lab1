{
  "name": "soksan-phork-lab1",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "mocha --exit",
    "test:coverage": "c8 --reporter=html --reporter=text --reporter=lcov mocha --exit",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "c8": {
    "exclude": [
      "test/**",
      "coverage/**",
      ".github/**",
      "eslint.config.js",
      "server.js"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "all": true,
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Soksan369/soksan-phork-lab1.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Soksan369/soksan-phork-lab1/issues"
  },
  "homepage": "https://github.com/Soksan369/soksan-phork-lab1#readme",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "c8": "^10.1.3",
    "chai": "^6.2.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "mocha": "^11.7.5",
    "supertest": "^7.1.4"
  }
}